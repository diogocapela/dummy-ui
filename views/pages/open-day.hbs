{{> header}}

<style>
    body {
        padding-top: 75px;
    }
</style>

<div class="nav-container-fixed">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="nav-back-btn" href="/">
            <i class="fas fa-arrow-left fa-lg"></i>
        </a>
        <a class="navbar-brand mr-auto" href="/">{{data.title}}</a>
    </nav>
</div>

<div class="container-fluid">
    <ul class="list-group list-group-flush">
        {{#each data.lists}}
            <a class="list-group-item" href="/open-day/{{../openDayId}}/{{@key}}">{{title}}</a>
        {{/each}}
    </ul>
</div>

{{> footer}}