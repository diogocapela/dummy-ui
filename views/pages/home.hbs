{{> header}}
{{> navbar}}

<div class="hero"></div>

<div class="container-fluid">

    <h1 class="section-title">Open Day '18</h1>

    <div class="carousel">
        {{#each open-days}}
            <a class="carousel-cell" href="/open-day/{{@key}}">
                <h4>{{title}}</h4>
            </a>
        {{/each}}
    </div>

    <h1 class="section-title">Graduate Program</h1>

    <div class="carousel">
        {{#each graduate-programs}}
            <a class="carousel-cell">
                <h4>{{title}}</h4>
            </a>
        {{/each}}
    </div>

    <h1 class="section-title">Meet Mindera Code & Culture</h1>

    <div class="carousel">
        {{#each code-and-culture}}
            <a class="carousel-cell">
                <h4>{{title}}</h4>
            </a>
        {{/each}}
    </div>

</div>

{{> footer}}